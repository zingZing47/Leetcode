class Solution:
    def moveZeroes(self, nums: List[int]) -> None:
        """
        Do not return anything, modify nums in-place instead.
        """
        cur = prev = 0
        
        while cur < len(nums):
            if nums[cur] != 0:                
                nums[prev] = nums[cur]
                prev += 1              
            cur += 1        

        for i in range (prev,len(nums)):
            nums[i] = 0
